generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       String @id @default(uuid())
  fullname String?
  password String
  email    String @unique
  phone    String @unique
  username String @unique
  permission  Int?
  projectName String?
// payment     String
}


